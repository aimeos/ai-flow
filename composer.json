{
	"name": "aimeos/ai-flow",
	"description": "Flow/NeosCMS adapter for Aimeos web shops and e-commerce solutions",
	"keywords": ["aimeos", "adapter", "typo3", "flow", "neoscms", "shop", "e-commerce"],
	"homepage": "https://aimeos.org/Flow",
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/ai-flow",
		"issues": "https://github.com/aimeos/ai-flow/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=5.3.3",
		"typo3/flow": "~2.0||~3.0",
		"composer/installers": "^1.0",
		"aimeos/aimeos-core": "2016.07.*",
		"zendframework/zend-diactoros": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\MAdmin\\": "lib/custom/src/MAdmin",
			"Aimeos\\MW\\": "lib/custom/src/MW"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
