{
	"name": "aimeos/ai-flow",
	"description": "Aimeos extension for utilizing TYPO3 Flow components",
	"keywords": ["aimeos", "adapter", "typo3", "flow"],
	"homepage": "http://aimeos.org",
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/ai-flow",
		"issues": "https://github.com/aimeos/ai-flow/issues",
		"forum": "https://help.aimeos.org",
		"wiki": "https://docs.aimeos.org"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"typo3/flow": "~2.0",
		"php": ">=5.3.2"
	},
	"autoload": {
		"psr-0": {
			"MW": "lib/custom/src"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
