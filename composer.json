{
	"name": "aimeos/ai-flow",
	"description": "Aimeos extension for utilizing TYPO3 flow components",
	"homepage": "http://aimeos.org",
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/ai-flow",
		"issues": "https://github.com/aimeos/ai-flow/issues"
	},
	"require": {
		"zendframework/zend-servicemanager": "2.3.*",
		"zendframework/zend-i18n": "2.3.*",
		"typo3/swiftmailer": "5.*",
		"typo3/flow": "2.2.*",
		"php": ">=5.3.2"
	},
	"autoload": {
		"psr-0": {
			"MW": "lib/custom/src"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}